{
    "priya": {
        "bin": "priya",
        "environment": "production",
        "version": "{$priya.major|default:0}.{$priya.minor|default:0}.{$priya.patch|default:0}",
        "contentType": {
            "txt": "text\/plain",
            "html": "text\/html",
            "css": "text\/css",
            "pcss": "text\/css",
            "js": "application\/javascript",
            "json": "application\/json",
            "ico": "image\/x-icon",
            "png": "image\/png",
            "jpg": "image\/jpeg",
            "gif": "image\/gif",
            "svg": "image\/svg+xml",
            "woff": "application\/font-woff",
            "ttf": "font\/ttf",
            "wav": "audio\/wav",
            "mp3": "audio\/mp3",
            "zip": "application\/zip",
            "rar": "application\/octet-stream"
        },
        "route": {
            "default": [
                "Application.Copyright",
                "Application.Version",
                "Application.License",
                "Application.Bin",
                "Application.Locate",
                "Application.Config",
                "Application.Help",
                "Application.Error",
                "Application.Route",
                "Application.Parser",
                "Application.Cache",
                "Application.Task",
                "Application.Host",
                "Test"
            ],
            "url": "{$dir.data}Route.json",
            "cache": {
                "url": "{$priya.dir.cache}60\/Route.json",
                "interval" : "+ 1 minute"
            }
        },
        "bug" : {
        	"cli" : {
        		"line" : {
        			"ending" : 2
        		}
        	},
        	"description" : {
        		"1" : "bug.cli.line.ending: 0: not adding newline character, 1: adding 1 newline character, 2: adding 2 newline characters. Default: 2. In CLI mode while echoing, newlines at the end don't work. Adding extra newlines work."
        	}
        },
        "major": 0,
        "minor": 3,
        "patch": 12,
        "built": "2018-11-17 23:38:36"
    }
}