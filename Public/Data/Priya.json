{
"@see" : "http://priya.software/Documentation/Json/Priya.json",
"name" : "{$url|basename:'.json'}",
"core" : "{$web.root}Priya/Public/Js/Prototype/Core/",
"prototype" : "{$core|dir.name}",
"extension" : ".prototype.js",
"require" : {
    "file" : [
        "{$prototype}Usleep{$extension}"
    ]
},
"method" : "append",
"target" : "body",
"template" : "{$name}.tpl",
"route" : "{route('priya-template', $template)}",
"html" : "{priya('{$web.root}Application/Template/{$template}', 'text/html', 'POST')}",
"script" : [
    "<script type=\"text/javascript\" src=\"{$web.root}Priya/Public/Template/{$name}.js\"></script>"
]
}