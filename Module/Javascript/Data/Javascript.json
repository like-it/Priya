{
"@see" : "http://priya.software/Documentation/Javascript",
"name" : "{$url|basename:'.json'}",
"core" : "{$web.root}Priya/Public/Js/Prototype/Core/",
"prototype" : "{$core|dir.name}",
"extension" : ".prototype.js",
"collect" : {
    "web" : "{object.merge($web,{prototype: $prototype, core: $core})}",
    "expose" : {
        "namespace" : "_",
        "require" : "require",
        "window" : {
            "isset" : "isset",
            "empty" : "empty",
            "microtime" : "microtime",
            "trim" : "trim",
            "run" : "run",
            "select" : "select",
            "get" : "get",
            "request" : "request",
            "attach" : "attach"
        }
    }
},
"require" : {
    "core" : [
        "{$core}Active{$extension}",
        "{$core}Append{$extension}",
        "{$core}Attach{$extension}",
        "{$core}Attribute{$extension}",
        "{$core}Basename{$extension}",
        "{$core}Calculate{$extension}",
        "{$core}Children{$extension}",
        "{$core}Class{$extension}",
        "{$core}Clone{$extension}",
        "{$core}Closest{$extension}",
        "{$core}Collection{$extension}",
        "{$core}Content{$extension}",
        "{$core}Create{$extension}",
        "{$core}Create.selector{$extension}",
        "{$core}Css{$extension}",
        "{$core}Data{$extension}",
        "{$core}Debug{$extension}",
        "{$core}Empty{$extension}",
        "{$core}Exception{$extension}",
        "{$core}Explode.multi{$extension}",
        "{$core}Explode{$extension}",
        "{$core}Extended{$extension}",
        "{$core}Find{$extension}",
        "{$core}Function.exists{$extension}",
        "{$core}Html{$extension}",
        "{$core}Implode{$extension}",
        "{$core}In.array{$extension}",
        "{$core}Is.array{$extension}",
        "{$core}Is.nodelist{$extension}",
        "{$core}Is.numeric{$extension}",
        "{$core}Isset{$extension}",
        "{$core}Jid{$extension}",
        "{$core}Link{$extension}",
        "{$core}Loader{$extension}",
        "{$core}Methods{$extension}",
        "{$core}Microtime{$extension}",
        "{$core}Namespace{$extension}",
        "{$core}Compare.natural{$extension}",
        "{$core}Next{$extension}",
        "{$core}Object.delete{$extension}",
        "{$core}Object.get{$extension}",
        "{$core}Object.horizontal{$extension}",
        "{$core}Object.merge{$extension}",
        "{$core}Object.set{$extension}",
        "{$core}Off{$extension}",
        "{$core}On{$extension}",
        "{$core}Previous{$extension}",
        "{$core}Rand{$extension}",
        "{$core}Refresh{$extension}",
        "{$core}Remove{$extension}",
        "{$core}Request{$extension}?{$priya.revision | default : $priya.version}",
        "{$core}Round{$extension}",
        "{$core}Run{$extension}",
        "{$core}Script{$extension}",
        "{$core}Scrollbar{$extension}",
        "{$core}Select{$extension}",
        "{$core}Str.replace{$extension}",
        "{$core}Str.stri{$extension}",
        "{$core}Style{$extension}",
        "{$core}Trigger{$extension}",
        "{$core}Trim{$extension}",
        "{$core}Val{$extension}"
    ],
    "file" : [
        "{$prototype}Usleep{$extension}"
    ]
}
}