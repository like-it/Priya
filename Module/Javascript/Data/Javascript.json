{
"@see" : "http://priya.software/Documentation/Javascript",
"name" : "{$url|basename:'.json'}",
"core" : "{$web.root}Priya/Public/Js/Prototype/Core/",
"prototype" : "{$core|dir.name}",
"web" : "{object.merge($web,{prototype: $prototype, core: $core})}",
"extension" : ".prototype.js",
"require" : {
    "core" : [
        "{$web.core}CLosest{$extension}",
        "{$web.core}Collection{$extension}",
        "{$web.core}Content{$extension}",
        "{$web.core}Debug{$extension}",
        "{$web.core}Empty{$extension}",
        "{$web.core}Exception{$extension}",
        "{$web.core}Isset{$extension}",
        "{$web.core}Microtime{$extension}",
        "{$web.core}Request{$extension}",
        "{$web.core}Run{$extension}",
        "{$web.core}Script{$extension}",
        "{$web.core}Select{$extension}",
        "{$web.core}Trim{$extension}"
    ],
    "file" : [
        "{$web.prototype}Usleep{$extension}"
    ]
}
}