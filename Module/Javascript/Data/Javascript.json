{
"@see" : "http://priya.software/Documentation/Javascript",
"name" : "{$url|basename:'.json'}",
"core" : "{$web.root}Priya/Public/Js/Prototype/Core/",
"prototype" : "{$core|dir.name}",
"extension" : ".prototype.js",
"collect" : {
    "web" : "{object.merge($web,{prototype: $prototype, core: $core})}",
    "expose" : {
        "namespace" : "_",
        "require" : "require",
        "window" : {
            "isset" : "isset",
            "empty" : "empty",
            "microtime" : "microtime",
            "trim" : "trim",
            "run" : "run",
            "select" : "select",
            "get" : "get",
            "request" : "request"
        }
    }
},
"require" : {
    "core" : [
        "{$core}CLosest{$extension}",
        "{$core}Collection{$extension}",
        "{$core}Content{$extension}",
        "{$core}Debug{$extension}",
        "{$core}Empty{$extension}",
        "{$core}Exception{$extension}",
        "{$core}Isset{$extension}",
        "{$core}Microtime{$extension}",
        "{$core}Request{$extension}",
        "{$core}Run{$extension}",
        "{$core}Script{$extension}",
        "{$core}Select{$extension}",
        "{$core}Trim{$extension}"
    ],
    "file" : [
        "{$prototype}Usleep{$extension}"
    ]
}
}